{
    "env": {
        "browser": true,
        "amd": true,
        "node": true
    },
    "extends": [
        "next/core-web-vitals",
        "prettier",
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["react", "react-hooks"],
    "rules": { "no-undef": "error" }
}
